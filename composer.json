{
    "name": "leyo/rapidussd",
    "description": "Rapid ussd package for quick ussd development and prototyping",
    "keywords": [
        "USSD",
        "Rapidussd"
    ],
    "homepage": "https://github.com/leyo/rapidussd",
    "license": "MIT",
    "authors": [
        {
            "name": "Leyo",
            "email": "leo@devs.mobi",
            "homepage": "http://devs.mobi",
            "role": "Developer"
        }
    ],
    "classmap": [
        "src/Http/controllers",
        "src/Http/migrations"
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "leyo\\rapidussd\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "leyo\\rapidussd\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
